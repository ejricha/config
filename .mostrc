% Override a few colors
color normal white black
color status black brightmagenta
color underline brightcyan black
color overstrike yellow black

% Keybindings
% (modified from from /usr/share/doc/most/lesskeys.rc)
unsetkey "G"
setkey goto_line ":"
unsetkey "^K"
setkey up "^K"
setkey bob "gg"
setkey eob "G"

unsetkey "c"
setkey toggle_case "c"

setkey down "e"
setkey down "E"
setkey down "j"
setkey down "^N"
setkey up "y"
setkey up "^Y"
setkey up "k"
setkey up "^P"
setkey page_down "f"
setkey page_down "^F"
setkey page_up "b"
setkey page_up "^B"
setkey other_window "z"
setkey other_window "w"
setkey search_backward "?"
setkey bob "p"
setkey goto_mark "'"
setkey find_file "E"
setkey edit "v"
setkey bob "^[[7~"
setkey eob "^[[8~"
setkey bob "^[OH"
setkey eob "^[OF"

